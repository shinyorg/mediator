{
  "openapi": "3.0.1",
  "info": {
    "title": "ShinyFleet.Api | v1",
    "version": "1.0.0"
  },
  "paths": {
    "/account/userinfo": {
      "get": {
        "tags": [
          "ShinyFleet.Api"
        ],
        "operationId": "GetUserInfo",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserInfo"
                }
              }
            }
          }
        }
      }
    },
    "/account/signup": {
      "post": {
        "tags": [
          "ShinyFleet.Api"
        ],
        "operationId": "SignUp",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SignUpRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ValidateResult"
                }
              }
            }
          }
        }
      }
    },
    "/push/register": {
      "post": {
        "tags": [
          "ShinyFleet.Api"
        ],
        "operationId": "PushRegistration",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterPush"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/backdoor": {
      "post": {
        "tags": [
          "ShinyFleet.Api"
        ],
        "operationId": "SignInBackdoor",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DevAuthRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/auth/refresh": {
      "post": {
        "tags": [
          "ShinyFleet.Api"
        ],
        "operationId": "RefreshAuthentication",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UseRefreshToken"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthResult"
                }
              }
            }
          }
        }
      }
    },
    "/auth/signin": {
      "post": {
        "tags": [
          "ShinyFleet.Api"
        ],
        "operationId": "SignIn",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthResult"
                }
              }
            }
          }
        }
      }
    },
    "/auth/signout": {
      "post": {
        "tags": [
          "ShinyFleet.Api"
        ],
        "operationId": "SignOut",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SignOut"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AuthRequest": {
        "required": [
          "email",
          "authCode"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "authCode": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "AuthResult": {
        "required": [
          "type"
        ],
        "type": "object",
        "properties": {
          "type": {
            "$ref": "#/components/schemas/AuthResultType"
          },
          "token": {
            "$ref": "#/components/schemas/AuthToken"
          }
        }
      },
      "AuthResultType": {
        "enum": [
          "Success",
          "TooManyRequests",
          "WaitForCode",
          "InvalidCode",
          "CodeExpired"
        ]
      },
      "AuthToken": {
        "required": [
          "value",
          "expires",
          "refreshToken"
        ],
        "type": "object",
        "properties": {
          "value": {
            "type": "string"
          },
          "expires": {
            "type": "string",
            "format": "date-time"
          },
          "refreshToken": {
            "type": "string"
          }
        },
        "default": null,
        "nullable": true
      },
      "DevAuthRequest": {
        "required": [
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        }
      },
      "PushPlatform": {
        "enum": [
          "Apple",
          "Google"
        ]
      },
      "RegisterPush": {
        "required": [
          "platform",
          "deviceToken"
        ],
        "type": "object",
        "properties": {
          "platform": {
            "$ref": "#/components/schemas/PushPlatform"
          },
          "deviceToken": {
            "type": "string"
          }
        }
      },
      "SignOut": {
        "type": "object"
      },
      "SignUpRequest": {
        "required": [
          "email",
          "phone",
          "firstName",
          "lastName",
          "inviteCode"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "inviteCode": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "UserCompany": {
        "required": [
          "id",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          }
        }
      },
      "UseRefreshToken": {
        "required": [
          "refreshToken"
        ],
        "type": "object",
        "properties": {
          "refreshToken": {
            "type": "string"
          }
        }
      },
      "UserInfo": {
        "required": [
          "id",
          "email",
          "phone",
          "firstName",
          "lastName",
          "companies"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "email": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "companies": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserCompany"
            }
          }
        }
      },
      "ValidateResult": {
        "required": [
          "errors"
        ],
        "type": "object",
        "properties": {
          "errors": {
            "type": "object",
            "additionalProperties": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "isValid": {
            "type": "boolean"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "ShinyFleet.Api"
    }
  ]
}